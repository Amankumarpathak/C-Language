#include<stdio.h>
int main(){
  int num1,num2;
  printf("Enter two numbers:");
  scanf("%d%d",&num1,&num2);
  printf("before swapping:\n");
  printf("num1=%d,num2=%d\n",num1,num2);
  num1=num1+num2;
  num2=num1-num2;
  num1=num1-num2;
 printf("after swapping:\n");
printf("num1=%d,num2=%d\n",num1,num2);
}
